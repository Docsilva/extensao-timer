{
    "manifest_version": 3,
    "name": "Minha Extensão",
    "version": "1.0",
    "description": "Descrição da extensão",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "notifications"
    ],
    "host_permissions": [
        "*://meet.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/pequeno.png",
        "48": "icons/medio.png",
        "128": "icons/grande.png"
    },
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["content.js"]
        }
    ]
}
